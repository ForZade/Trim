# Ignore node_modules directory
node_modules/

# Ignore .env files (environment variables)
.env
.env.*  # In case you use different environments like .env.development, .env.production, etc.

# Ignore logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore OS-generated files
.DS_Store
Thumbs.db

# Ignore IDE files
.vscode/
.idea/
*.sublime-workspace
*.sublime-project

# Ignore npm package locks (optional if using yarn)
package-lock.json
yarn.lock

# Ignore compiled files
dist/
build/

# Ignore coverage reports
coverage/

# Ignore TypeScript build files
*.tsbuildinfo

# Ignore eslint cache files
.eslintcache

# Ignore temporary files
*.swp
*.tmp
*.bak
*.backup
